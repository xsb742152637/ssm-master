<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="model.core.menuurl.dao.CoreMenuUrlInfoDao">
    <!--表名-->
    <sql id="tableName">core_menu_url_info</sql>
    <!--主键-->
    <sql id="primaryKey">URL_ID</sql>

    <!--auto generated Code-->
    <sql id="all_column">
        `url_id`,
        `title`,
        `code`,
        `url`,
        `parameter`,
        `sys_time`
    </sql>

    <sql id="_main_info">
        select main.* from <include refid="tableName"/> main
        where 1=1
        <if test="mainId != null">
            and main.<include refid="primaryKey"/> = #{mainId},
        </if>
        <if test="searchKey != null">
            and (locate(#{searchKey},main.TITLE)>0 or locate(#{searchKey},main.CODE)>0)
        </if>
    </sql>

    <select id="getMainInfo" resultType="CoreMenuUrlInfoEntity">
        <include refid="_main_info"/>
        ORDER BY main.SYS_TIME desc
        <if test="rows != -1">
            LIMIT ${(page-1)*rows},${rows};
        </if>
    </select>
    <select id="getMainCount" resultType="Integer">
        select count(1) from (<include refid="_main_info"/>) c
    </select>

    <select id="findOne" resultType="CoreMenuUrlInfoEntity" parameterType="String">
        <include refid="GenericMapper.findOne"/>
    </select>

    <delete id="delete" parameterType="String">
        <include refid="GenericMapper.delete"/>
    </delete>

    <!--auto generated Code-->
    <insert id="insertList">
        INSERT INTO core_menu_url_info (
        <include refid="all_column"/>
      )VALUES
        <foreach collection="list" item="entity" index="index" separator=",">
            (
            #{entity.urlId},
            #{entity.title},
            #{entity.code},
            #{entity.url},
            #{entity.parameter},
            #{entity.sysTime}
            )
        </foreach>
    </insert>



    <!--auto generated Code-->
    <update id="update">
        UPDATE core_menu_url_info
        <set>
            <if test="entity.urlId != null"> `url_id` = #{entity.urlId}, </if>
            <if test="entity.title != null"> `title` = #{entity.title}, </if>
            <if test="entity.code != null"> `code` = #{entity.code}, </if>
            <if test="entity.url != null"> `url` = #{entity.url}, </if>
            <if test="entity.parameter != null"> `parameter` = #{entity.parameter}, </if>
            <if test="entity.sysTime != null"> `sys_time` = #{entity.sysTime} </if>
        </set>
        WHERE url_id = #{entity.urlId}
    </update>

</mapper>

