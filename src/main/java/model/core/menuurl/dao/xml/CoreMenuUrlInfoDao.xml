<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="model.core.menuurl.dao.CoreMenuUrlInfoDao">
    <!--表名-->
    <sql id="tableName">core_menu_url_info</sql>
    <!--主键-->
    <sql id="primaryKey">URL_ID</sql>

    <!--auto generated Code-->
    <sql id="all_column">
        `url_id`,
        `title`,
        `code`,
        `url`,
        `parameter`,
        `sys_time`
    </sql>

    <sql id="_main_info">
        select main.* from <include refid="tableName"/> main
        where 1=1
        <if test="mainId != null">
            and main.<include refid="primaryKey"/> = #{mainId},
        </if>
        <if test="searchKey != null">
            and (locate(#{searchKey},main.TITLE)>0 or locate(#{searchKey},main.CODE)>0)
        </if>
    </sql>

    <select id="getMainInfo" resultType="CoreMenuUrlInfoEntity">
        <include refid="_main_info"/>
        ORDER BY main.SYS_TIME desc
        <if test="rows != -1">
            LIMIT ${(page-1)*rows},${rows};
        </if>
    </select>
    <select id="getMainCount" resultType="Integer">
        select count(1) from (<include refid="_main_info"/>) c
    </select>

    <select id="findOne" resultType="CoreMenuUrlInfoEntity" parameterType="String">
        <include refid="GenericMapper.findOne"/>
    </select>

    <delete id="delete" parameterType="String">
        <include refid="GenericMapper.delete"/>
    </delete>

    <!--auto generated Code-->
    <insert id="insert" useGeneratedKeys="true" keyProperty="pojo.urlId">
        INSERT INTO core_menu_url_info (
        <include refid="all_column"/>
        ) VALUES (
        #{pojo.urlId},
        #{pojo.title},
        #{pojo.code},
        #{pojo.url},
        #{pojo.parameter},
        #{pojo.sysTime}
        )
    </insert>

    <!--auto generated Code-->
    <insert id="insertList">
        INSERT INTO core_menu_url_info (
        <include refid="all_column"/>
      )VALUES
        <foreach collection="pojos" item="pojo" index="index" separator=",">
            (
            #{pojo.urlId},
            #{pojo.title},
            #{pojo.code},
            #{pojo.url},
            #{pojo.parameter},
            #{pojo.sysTime}
            )
        </foreach>
    </insert>



    <!--auto generated Code-->
    <update id="update">
        UPDATE core_menu_url_info
        <set>
            <if test="pojo.urlId != null"> `url_id` = #{pojo.urlId}, </if>
            <if test="pojo.title != null"> `title` = #{pojo.title}, </if>
            <if test="pojo.code != null"> `code` = #{pojo.code}, </if>
            <if test="pojo.url != null"> `url` = #{pojo.url}, </if>
            <if test="pojo.parameter != null"> `parameter` = #{pojo.parameter}, </if>
            <if test="pojo.sysTime != null"> `sys_time` = #{pojo.sysTime} </if>
        </set>
        WHERE url_id = #{pojo.urlId}
    </update>

</mapper>

